{
    "name": "zendframework/zendframework1",
    "description": "Zend Framework 1",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-0": {
            "Zend_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "archive": {
        "exclude": ["/demos", "/documentation", "/tests"]
    }
}
